import java.util.Scanner;
public class Leap
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int year=sc.nextInt();
        if(year%4==0)
        {
            System.out.println("given year is leap year");
        }
        else
        {
            System.out.println("given year is not a leap year"); 
        }
    }
}
